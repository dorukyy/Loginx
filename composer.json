{
    "name": "dorukyy/loginx",
    "type": "library",
    "description": "Whole login system for Laravel",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^v10.10.0",
        "laravel/tinker": "^2.9"
    },
    "authors": [
        {
            "name": "dorukyy",
            "email": "dorukydemirel@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "dorukyy\\loginx\\": "src/",
            "dorukyy\\loginx\\database\\seeders\\": "database/seeders/",
            "dorukyy\\loginx\\database\\factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "dorukyy\\loginx\\LoginxServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-package-update": [
            "php artisan vendor:publish --provider=dorukyy\\loginx\\LoginxServiceProvider --force"
        ],
        "post-package-install": [
            "php artisan vendor:publish --provider=dorukyy\\loginx\\LoginxServiceProvider --force"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "version": "1.5",
    "minimum-stability": "dev",
    "prefer-stable": true
}
